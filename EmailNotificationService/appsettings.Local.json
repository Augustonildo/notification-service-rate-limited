{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "ServiceBusConnection": "your-local-servicebus-connection-string"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information"
    }
  },
  "RateLimitTypeList": [
    {
      "Type": "Status",
      "HasRateLimit":  true,
      "TimeRangeInMinutes": 1,
      "NotificationLimit": 2
    },
    {
      "Type": "News",
      "HasRateLimit": true,
      "TimeRangeInMinutes": 1440,
      "NotificationLimit": 1
    },
    {
      "Type": "Marketing",
      "HasRateLimit": true,
      "TimeRangeInMinutes": 60,
      "NotificationLimit": 3
    },
    {
      "Type": "Security Breach",
      "HasRateLimit": false
    }
  ]
}